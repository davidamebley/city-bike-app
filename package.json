{
  "name": "city-bike-app",
  "version": "1.0.0",
  "description": "An app that displays a User Interface for showing data from journeys made with city bikes",
  "main": "server.ts",
  "scripts": {
    "start": "node backend/server.js",
    "build": "tsc",
    "server": "nodemon backend/server.ts",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "REACT_APP_SERVER_URL=$REACT_APP_SERVER_URL NPM_CONFIG_PRODUCTION=false npm install --prefix frontend && REACT_APP_SERVER_URL=$REACT_APP_SERVER_URL npm run build --prefix frontend",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidamebley/city-bike-app.git"
  },
  "author": "David Amebley",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/davidamebley/city-bike-app/issues"
  },
  "homepage": "https://github.com/davidamebley/city-bike-app#readme",
  "dependencies": {
    "aws-sdk": "^2.1377.0",
    "body-parser": "^1.20.2",
    "concurrently": "^7.6.0",
    "cors": "^2.8.5",
    "csv-parse": "^5.3.6",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "fs": "^0.0.1-security",
    "mongodb": "^5.1.0",
    "mongoose": "^7.0.2",
    "node-cache": "^5.1.2",
    "typescript": "^5.0.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "nodemon": "^2.0.21"
  }
}
